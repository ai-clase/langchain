{
  "name": "@aimpact/langchain",
  "version": "1.0.0",
  "title": "lang chain package",
  "description": "",
  "license": "ISC",
  "layout": "main-layout",
  "template": "template/template.json",
  "languages": {
    "default": "en",
    "supported": [
      "en",
      "es"
    ]
  },
  "modules": {
    "path": "modules"
  },
  "libraries": {
    "imports": [
      "@beyond-js/reactive-2"
    ]
  },
  "deployment": {
    "distributions": [
      {
        "name": "web",
        "environment": "development",
        "platform": "web",
        "backend": "backend",
        "ports": {
          "bundles": 3030
        }
      },
      {
        "name": "backend",
        "environment": "development",
        "platform": "backend",
        "maps": "external",
        "ports": {
          "bundles": 3034,
          "inspect": 3035,
          "http": 3036
        }
      },
      {
        "name": "node",
        "environment": "development",
        "platform": "node",
        "maps": "external",
        "ports": {
          "bundles": 3031,
          "inspect": 3032,
          "http": 3033
        }
      }
    ]
  },
  "dependencies": {
    "@beyond-js/backend": "~0.1.6",
    "@beyond-js/kernel": "~0.1.8",
    "@beyond-js/local": "~0.1.3",
    "@beyond-js/react-18-widgets": "~0.0.1",
    "@pinecone-database/pinecone": "^0.1.5",
    "@google-cloud/storage": "^6.10.1",
    "socket.io-client": "^4.5.4",
    "dotenv": "^16.0.3",
    "openai": "^3.2.1",
    "langchain": "^0.0.75",
    "mammoth": "^1.5.1",
    "pdf-parse": "1.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9"
  }
}
